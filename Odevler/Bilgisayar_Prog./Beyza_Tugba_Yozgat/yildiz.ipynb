        "def draw_star(n):\n",
        "    for i in range(n):\n",
        "        print(' '*(n-i-1) + '*'*(2*i+1))\n",
        "    for i in range(n-2, -1, -1):\n",
        "        print(' '*(n-i-1) + '*'*(2*i+1))\n",
        "\n",
        "draw_star(5)"
